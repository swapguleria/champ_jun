}

/*============== newsletter =============*/
.newsletter {
	float:left;
	width:100%;
	text-align:center;
	margin: 60px 0;
	padding: 30px 0;
	background:rgba(245, 243, 244, 0.5);
}
.news-inner {
	display: inline-block;
    margin: 0 auto;
    width: 960px;
}
.news-inner h1 {
	color: rgb(196, 158, 96);
    font-family: "italianno";
    font-size: 42px;
    text-align: center;
	float:left;
    text-transform: capitalize;
}

.newsletter form,
.login-inner .login-icons{
    float: right;
    margin:26px 0 10px;
}
.newsletter form input,
.login-icons input{
    border: 1px solid rgb(230, 187, 116) !important;
    border-radius: 50px !important;
	color:#616161;
	font-family: 'Merriweather';
	font-size:12px;
	height: 41px;
	background: rgba(0, 0, 0, 0);
    margin: 0 10px 0 0 !important;
    width: 250px !important;
}
.newsletter form .btn-default,
.confirmation-content .about-content a,
.login-form.form .login-btn{
    background-color: rgb(8, 8, 8);
    color:#eeeeee;
	border:0 !important;
	font-family: "Proxima Nova Lt";
	font-size:15px;
	border-radius:50px;
	text-transform:uppercase;
	padding: 10px 25px;
	margin: 0 0 0 17px;
}
.newsletter form .btn-default:hover,
.perfect-blend a:hover,
.confirmation-content .about-content a:hover{
	background:#c6a164;
}
.social-icon {
    float: left;
    width: 100%;
}
.social {
    float: left;
    text-align: center;
    width: 100%;
}
.social > ul {
