#carousel .slides{
	background:url(../images/thumb-bg.png);
	background-repeat:no-repeat;
	background-position:20px 35px;
	background-size:1120px;
	margin: 0 auto;
    width: 1140px !important;
}*/
.flexslider ul{
	padding:0;
	margin:0;
}
.flexslider ol{
	margin:0;
	padding:0;
	text-align:center;
	position:absolute;
	z-index:9;
	width:100%;
	bottom:44px;
}
.flex-control-nav li {
    margin: 0 6px;
    display: inline-block;
    zoom: 1;
}
.flex-control-nav li:last-child{
	margin:0 0 0 6px;
}
.flex-control-paging li a {
    width: 11px;
    height: 11px;
    display: block;
    cursor: pointer;
    text-indent: -9999px;
    border-radius: 20px;
	border:1px solid #f2f2f2;
}
.flex-control-paging li a.flex-active{
	border:1px solid #bc975c;
}
.flexslider ul li{
	list-style:none;
	position:relative;
	border-radius:50%;
}
#slider.flexslider ul.flex-direction-nav,
.our-team .flex-direction-nav{
	display:none;
}
#slider.flexslider ul li img,
#carousel.flexslider ul{
	width:100% !important;
}
/*.site-banner #carousel{
	position: absolute;
    bottom: 110px;
    width: 100%;
}*/
#carousel.flexslider ul li{
	width: auto !important;
    float: none !important;
    display: inline-block !important;
	margin:0 35px 0 0 !important;
	cursor:pointer;
}
#carousel.flexslider ul li:last-child{
	margin:0 !important;
}
#carousel.flexslider ul{
	text-align:center;
	width:1140px !important;
}

/*============== 1-Aug-2016 ===================*/
#sync2 span,
#carousel ul span{
	color:#eee;
	font-family: 'Merriweather Sans', sans-serif;
	font-weight:400;
	font-style:italic;
	font-size:16px;
	margin:0 42px 0 0;
	position:relative;
}
#sync2 span:after,
#carousel ul span:after{
	background:url(../images/arrows.png) no-repeat;
	height:16px;
	width:12px;
	position:absolute;
	content:'';
	right:-20px;
	top:3px;
}
.carousel-caption h1,
.carousel-caption h2,
.banner-caption h1,
.banner-caption h2{
	margin:0;
	font-size:72px;
	color:#c49e60;
	font-family: "italianno";
}
.carousel-caption h2,
.banner-caption h2{
	text-transform:uppercase;
	font-size:74px;
	color:#eee;
	margin: 2px 0 35px;
	font-family: 'Proxima Nova';
	font-weight:bold;
}
.carousel-caption a,
.banner-caption a{
	text-decoration:none;
	text-transform:uppercase;
	color:#fff;
	font-size:15px;
	font-family: 'Proxima Nova Lt';
	font-weight:600;
	border:1px solid #e6bb74;
	border-radius:50px;
	padding:16px 50px;
	display:inline-block;
}
.carousel-caption a:hover,
.banner-caption a:hover{
	border:1px solid #9b7c49;
	background: #c6a164;
}
#slider .carousel-caption,
.site-banner .banner-caption{
	top:228px;
	left:auto;
	right:auto;
	bottom:auto;
	padding:0;
	width:100%;
	position:absolute;
	top:18;
}
.carousel-caption p,
.reserve-timings p{
	font-size:15px;
	font-family: 'Merriweather-BlackItalic';
	letter-spacing:5px;
	font-style:italic;
	margin:0 0 30px;
}


/*========== About Us Starts Here============= */
.about-us {
	background-size:cover !important;
	background-position:top right !important;
	float:left;
	width:100%;
}
.inner-about {
    float: left;
    width: 100%;
	text-align:center;
}
.inner-heading {
    float: left;
    padding: 56px 0;
    width: 100%;
}
.inner-heading h1,
.form-details h2,
.about-site h1,
.login-inner h2.login-title{
	color: rgb(196, 158, 96);
    font-family: "italianno";
    font-size: 50px;
    text-align: center;
    text-transform: capitalize;
	margin:0;
}
.inner-heading p{
    font-family: 'Proxima Nova Rg';
    font-size: 13px;
    letter-spacing: 2px;
    text-align: center;
	width:53%;
	position:relative;
	margin:0 auto;
    text-transform: uppercase;
    word-spacing: 3px;
}
.inner-heading p:before,
.overlay-section p:before{
	background:rgb(196, 158, 96);
	bottom: 8px;
    content: " ";
    height: 1px;
    left: 31px;
    position: absolute;
    width: 137px;
}
.inner-heading p:after,
.overlay-section p:after{
	background:rgb(196, 158, 96);
	bottom: 8px;
    content: " ";
    height: 1px;
    right: 31px;
    position: absolute;
    width: 137px;
}
.text-area {
	float:left;
	width:100%;
	text-align:center;
}
.middle-content {
    display: inline-block;
    font-family: "Merriweather";
    font-size: 16px;
    line-height: 40px;
	color:#171717;
    width: 67%;
	font-style:italic;
	line-height:33px;
}
/*background-position: -8.6% 0.5%;*/
.block-area {
	float:left;
	width:100%;
	padding: 50px 0;
	text-align:center;
}
.block-inner-section {
	 width:890px;
	 display:inline-block;
	 margin:0 auto;
}
.block-inner-section .col-sm-6:first-child .book-detail .form-control,
.book-detail form .timepicker{
	background:#fff !important;
	padding:6px 15px;
}
.block-inner-section .col-sm-6:first-child .book-detail .form-control,
.block-inner-section .col-sm-6:last-child .registration-date #registration-date{
	background:url(../images/calendar-icon.png) no-repeat #fff !important;
	background-position:95% !important;
}
.block-inner-section .col-sm-6:last-child .book-detail{
	background:#fff !important;
}
.block-inner-section .col-sm-6 .book-detail .form-control,
.block-inner-section .col-sm-6:last-child .book-detail select.form-control,
.book-detail form .timepicker,
.book-detail form .checkbox,
.detail-form .form-control{
	background:#f7f7f7 !important;
	border-radius:20px;
}
.block-inner-section .col-sm-6:first-child .book-detail select.form-control,
.block-inner-section .col-sm-6:last-child .book-detail select.form-control,
.block-inner-section .col-sm-6:last-child .book-detail select.form-control{
	background:url(../images/caret.png) no-repeat #fff !important;
	background-size:16px 9px !important;
	-webkit-appearance:none;
	-moz-appearance:none;
    appearance:none;
	background-position:96% 61% !important;
	cursor:pointer;
	padding:6px 15px;
}
.block-inner-section .col-sm-6:first-child .book-detail .form-group:nth-child(2n) select.form-control,
.block-inner-section .col-sm-6:last-child .book-detail .form-group:nth-child(5n) select.form-control{
	background:url(../images/time-icon.png) no-repeat #fff !important;
	background-position:95% !important;
}
.block-inner-section .col-sm-6:last-child .book-detail select.form-control,
.block-inner-section .col-sm-6:last-child .book-detail select.form-control{
	background:url(../images/caret.png) no-repeat #f7f7f7 !important;
	background-position:96% 61% !important;
}
.block-inner-section .country-code,
.form-details .country-code{
	background:url(../images/caret.png) no-repeat #f7f7f7 !important;
	background-position:96% 61% !important;
	height:0;
}
.form-details .form-group .form-control:nth-child(3n) .form-control{
	background:url(../images/lang-caret.png) no-repeat #f7f7f7 !important;
	background-size:7px 4px !important;
	background-position: 89% !important;
}
.block-inner-section .country-code ul,
.form-details .country-code ul{
	margin:0;
	padding:0;
	width: 100px;
    position: relative;
    top: -33px;
    right: -80%;
}
.block-inner-section .country-code ul li,
.form-details .country-code ul li{
	list-style:none;
}
.img-section {
	float:left;
	width:100%;
	position:relative;
}
.img-section img {
	float:left;
	width:100%;
}
.overlay-section {
    position: absolute;
    width: 100%;
	padding: 21px 0;
}
.overlay-section h1 {
	color: rgb(196, 158, 96);
    font-family: "italianno";
    font-size: 50px;
    text-align: center;
    text-transform: capitalize;
}
.overlay-section p {
	font-family: 'Proxima Nova Rg';
    font-size: 13px;
    letter-spacing: 1px;
    text-align: center;
	position:relative;
	margin:0 auto;
	color:#ccc;
    text-transform: uppercase;
    word-spacing: 3px;
}
.overlay-section p:before,
.overlay-section p:after{
	background:#fff;
	width:63px;
}
.overlay-section p:before{
	left:22px;
}
.overlay-section p:after{
	right:22px;
}
.overlay-section a {
	text-decoration:none;
	text-transform:uppercase;
	color:#fff;
	font-size:12px;
	font-family: 'Proxima Nova Lt';
	font-weight:normal;
	border:1px solid #e6bb74;
	border-radius:50px;
	padding:11px 60px 11px 20px;
	display:inline-block;
}
.view {
    display: inline-block;
    margin: 20px auto;
    position: relative;
    width: auto;
}
.img-section .overlay-section:hover .view a{
	border-color:#9b7c49;
}
.img-section .overlay-section:hover .view span{
	background:#b49259;
}
.view > span {
    background: rgb(196, 158, 96) none repeat scroll 0 0;
    border-radius: 100px;
    float: right;
    height: 39px;
    position: absolute;
    right: 1px;
    top: 1px;
    width: 39px;

}
.view > span:hover{
	background: #c6a164;
}
.view > span a {
	border:none;
	padding: 13px 0 0 4px;
}
/*=========== perfect-blend-section ===========*/
.blend-content {
    text-align: center;
}
.perfect-blend {
	float:left;
	width:100%;
	padding: 30px 0;
	background-size:cover !important;
}
.perfect-blend h1 {
	color: rgb(196, 158, 96);
    font-family: "italianno";
    font-size: 50px;
    text-align: center;
	padding-bottom: 20px;
    text-transform: capitalize;
}
.perfect-blend p {
	color: rgb(238, 238, 238);
    font-family: "Proxima Nova Lt";
    font-size: 64px;
    font-weight: bold;
    margin: 10px 0 20px;
	text-align: center;
	position:relative;
	width:55%;
	margin:0 auto 10px;
    text-transform: uppercase;
}
.perfect-blend p:before {
	background:rgb(196, 158, 96);
	bottom: 47px;
    content: " ";
    height: 1px;
    left: 55px;
    position: absolute;
    width: 137px;
}
.perfect-blend p:after {
	background: rgb(196, 158, 96) none repeat scroll 0 0;
    bottom: 47px;
    content: " ";
    height: 1px;
    position: absolute;
    right: 55px;
    width: 137px;
}
.perfect-blend a {
	border: 1px solid rgb(230, 187, 116);
    border-radius: 50px;
    color: rgb(255, 255, 255);
    display: inline-block;
    font-family: "italianno";
    font-size: 23px;
    padding: 10px 55px;
    text-decoration: none;
}

