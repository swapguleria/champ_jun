body{
	margin:0;
	font-size:13px;
	color:#171717;
}
a,
.carousel-caption a:hover,
.newsletter form .btn-default,
.img-section .overlay-section:hover .view span,
.social ul li a img:hover,
.block-inner-section .btn-default:hover,
.form-details .btn-default:hover,
.contact-form .btn-default:hover,
.team-section .team-block:hover .team-content{
	outline:0!important;
	-webkit-transition:all .3s ease-in;
	-moz-transition:all .3s ease-in;
	-o-transition:all ease-in;
	transition:all .3s ease-in;
}
img{
	border:0;
}
.container{
	max-width:1170px;
	margin:0 auto;
}

/*============ Home Page Starts Here ==============*/

/*=============== Header Starts Here ===============*/
.site-header{
	border-top:3px solid #c49e60;
	z-index:2;
	padding:25px 0;
	position:fixed;
	width:100%;
	z-index:9999;
}
.site-header.header-change{
	background:url(../images/header-bg.png) no-repeat;
	background-color:#000;
	background-size:cover;
}
.site-header .container{
	padding:0;
}
.site-header .main-menu{
	padding:39px 0 0;
}
.main-menu .navbar-default{
	margin:0;
	border:0;
	background:none;
}
.main-menu .navbar-collapse{
	padding:0;
}
.main-menu .navbar-collapse ul,
.main-menu .navbar-collapse ul li{
	float:none;
}
.main-menu .navbar-collapse ul li{
	padding:0 21px;
	display:inline-block;
}
.main-menu .navbar-collapse ul li a{
	text-transform:uppercase;
	font-family: 'Varela Round', sans-serif;
	font-weight:400;
	color:#fff;
	padding:0 3px 9px;
}
.main-menu .navbar-collapse ul li.active a{
	background:none !important;
	color: #c49e60 !important;
    border-bottom: 3px solid #c49e60 !important;
}
.main-menu .navbar-collapse ul li a:hover{
	color:#c49e60;
	border-bottom:3px solid #c49e60;
	background:none;
}

/*=============== Banner Starts Here ===============*/
.site-banner{
	position:relative;
	z-index:1;
	float:left;
	width:100%;
}
.site-banner #sync2{
	bottom: 110px;
    left: 0;
    position: absolute;
}
.site-banner #sync2 .owl-item,
{
	float:none;
	display:inline-block;
	width:auto !important;
}
.site-banner #sync2 .item{
    cursor: pointer;
	margin:0 26px 0 0;
}
.site-banner #sync2 .owl-item:first-child {
}
.site-banner #sync2 .owl-item:last-child .item{
	margin:0;
}
.owl-carousel .owl-controls{
	display:none !important;
}
.site-banner .owl-item{
	width:100% !important;
}
.site-banner .owl-item img{
	width:100%;
}
.site-banner .owl-wrapper{
	width:auto !important;
	text-align:center;
}
#sync2 .owl-item img{
	width:auto;
}
.owl-carousel {
	width:100% !important;
	text-align:center;
}
